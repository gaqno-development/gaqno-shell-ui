{
  "extends": "@gaqno-development/frontcore/config/tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve",
    "skipLibCheck": true,
    "types": [
      "vite/client"
    ],
    "strictNullChecks": true
  },
  "include": [
    "src/vite-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
